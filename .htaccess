RewriteRule ^.*-itm-(\d+)/.+.xhtml$ index\.php?id=$1&%{QUERY_STRING} [L]
RewriteRule ^catalog-.+-(\d+).xhtml$ index\.php?cat=$1&%{QUERY_STRING} [L]
RewriteRule ^.*(site[a-z]+map.*\.xml)$  Site_map/$1 [L]
RewriteRule ^.*(sitemap\.xml)$  Site_map/$1 [L]

RewriteRule ^.*-itm-(\d+)/.+.xhtml$ index\.php?id=$1&%{QUERY_STRING} [L]
RewriteRule ^catalog-.+-(\d+).xhtml$ index\.php?cat=$1&%{QUERY_STRING} [L]
RewriteRule ^.*(site[a-z]+map.*\.xml)$  Site_map/$1 [L]
RewriteRule ^.*(sitemap\.xml)$  Site_map/$1 [L]

RewriteRule ^.*-itm-(\d+)/.+.xhtml$ index\.php?id=$1&%{QUERY_STRING} [L]
RewriteRule ^catalog-.+-(\d+).xhtml$ index\.php?cat=$1&%{QUERY_STRING} [L]
RewriteRule ^.*(site[a-z]+map.*\.xml)$  Site_map/$1 [L]
RewriteRule ^.*(sitemap\.xml)$  Site_map/$1 [L]

RewriteBase /
</IfModule>